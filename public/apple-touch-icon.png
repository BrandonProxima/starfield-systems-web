<!-- This would be a PNG file, but I'll create an SVG version for now -->
<svg width="180" height="180" viewBox="0 0 180 180" xmlns="http://www.w3.org/2000/svg">
    <defs>
        <radialGradient id="appleLogo" cx="50%" cy="50%" r="50%">
            <stop offset="0%" style="stop-color:#F59E0B;stop-opacity:0.8" />
            <stop offset="100%" style="stop-color:#F59E0B;stop-opacity:0" />
        </radialGradient>
        <filter id="appleGlow">
            <feGaussianBlur stdDeviation="3"/>
        </filter>
    </defs>
    
    <!-- Background with slight rounding for iOS -->
    <rect width="180" height="180" rx="20" fill="#0A0A0B"/>
    
    <!-- Particles -->
    <circle cx="40" cy="30" r="2" fill="rgba(255,255,255,0.6)"/>
    <circle cx="140" cy="50" r="1.5" fill="rgba(255,255,255,0.4)"/>
    <circle cx="30" cy="100" r="2" fill="rgba(245,158,11,0.8)"/>
    <circle cx="150" cy="120" r="1.5" fill="rgba(255,255,255,0.5)"/>
    <circle cx="60" cy="150" r="2" fill="rgba(255,255,255,0.3)"/>
    <circle cx="120" cy="40" r="1.5" fill="rgba(245,158,11,0.6)"/>
    
    <!-- Glow effect behind S -->
    <circle cx="90" cy="90" r="60" fill="url(#appleLogo)" filter="url(#appleGlow)"/>
    
    <!-- S letter -->
    <text x="90" y="120" 
          font-family="serif" 
          font-size="80" 
          font-weight="900" 
          font-style="italic" 
          fill="#FFFFFF" 
          text-anchor="middle">S</text>
</svg>